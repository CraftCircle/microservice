      "entryFile": "main",
      "sourceRoot": "apps/events/src",
      "compilerOptions": {
        "tsConfigPath": "apps/events/tsconfig.app.json"
      }
    },
    "posts": {
      "type": "application",
      "root": "apps/posts",
      "entryFile": "main",
      "sourceRoot": "apps/posts/src",
      "compilerOptions": {
        "tsConfigPath": "apps/posts/tsconfig.app.json"
      }
    },
    "tickets": {
      "type": "application",
      "root": "apps/tickets",
      "entryFile": "main",
      "sourceRoot": "apps/tickets/src",
      "compilerOptions": {
